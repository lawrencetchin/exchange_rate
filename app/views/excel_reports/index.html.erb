<p id="notice"><%= notice %></p>

<h1>Excel Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @excel_reports.each do |excel_report| %>
      <tr>
        <td><%= link_to 'Show', excel_report %></td>
        <td><%= link_to 'Edit', edit_excel_report_path(excel_report) %></td>
        <td><%= link_to 'Destroy', excel_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Excel Report', new_excel_report_path %>
